              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 4,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "id": 4,
      "options": {
        "footer": {
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "mariadb_exporter_build_info{job=\"$job\",instance=\"$instance\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Exporter Build Info",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Value": true,
              "job": true,
              "__name__": true
            },
            "renameByName": {
              "version": "Version",
              "commit": "Commit",
              "arch": "Architecture"
            }
          }
        }
      ],
      "type": "table"
    }
